{
  "name": "dayrade-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Dayrade Frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/Asad13/example.git"
  },
  "author": {
    "name": "Asad Mujumder",
    "email": "asaduzzaman.anik.me@gmail.com",
    "url": "https://asad13.github.io"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "i18next:interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/types/resources.d.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{ts,tsx,css,md}": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "eslint",
    "stylelint",
    "prettier",
    "typescript",
    "redux",
    "postcss"
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "packageManager": "yarn@1.22.21",
  "browserslist": [
    "last 2 versions",
    "> 0.2%",
    "not dead"
  ],
  "dependencies": {
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "^5.28.10",
    "axios": "^1.6.5",
    "date-fns": "^3.1.0",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "i18next-resources-for-ts": "^1.4.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^16.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^9.3.0",
    "postcss-reporter": "^7.0.5",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
